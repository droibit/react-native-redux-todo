{
  "name": "react-native-redux-todo",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "lint": "tslint 'src/**/*.ts{,x}'",
    "i18n-ts": "i18n-ts",
    "i18n-ts:watch": "i18n-ts -w",
    "test": "jest"
  },
  "dependencies": {
    "flux-standard-action": "^2.0.3",
    "immutable": "^3.8.2",
    "moment": "^2.22.2",
    "native-base": "2.6.1",
    "react": "16.3.1",
    "react-native": "0.55.4",
    "react-native-config": "^0.11.5",
    "react-native-i18n": "^2.0.12",
    "react-native-navigation": "^1.1.474",
    "react-native-vector-icons": "^4.6.0",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "reselect": "^3.0.1",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/jest": "^23.1.4",
    "@types/react": "^16.4.6",
    "@types/react-native": "^0.55.27",
    "@types/react-native-navigation": "^1.1.12",
    "@types/react-native-vector-icons": "^4.6.1",
    "@types/react-redux": "^6.0.3",
    "@types/react-test-renderer": "^16.0.1",
    "@types/uuid": "^3.4.3",
    "babel-jest": "23.2.0",
    "babel-preset-react-native": "4.0.0",
    "jest": "23.2.0",
    "react-native-i18n-ts": "^0.1.1",
    "react-native-typescript-transformer": "^1.2.10",
    "react-test-renderer": "16.3.1",
    "ts-jest": "^23.0.0",
    "typescript": "^2.9.2"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(js)$": "<rootDir>/node_modules/babel-jest",
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/"
    ],
    "cacheDirectory": ".jest/cache"
  },
  "react-native-i18n-ts": {
    "model": "./src/locale/languages/en.json",
    "outputDir": "./typings"
  }
}
